/*Gripper functions. Oly for Ch*/
int CLinkbotI::openGripper(double angle) //angle is in degrees
{
    return moveTo(-angle/2, NaN, -angle/2);
}

int CLinkbotI::openGripperNB(double angle) 
{
    return moveToNB(-angle/2, NaN, -angle/2);
}

int CLinkbotI::closeGripper(void) 
{
    double gripperAngleOld= 0;
    double gripperAngleNew;
    int retval;

    retval = getJointAngle(ROBOT_JOINT1, gripperAngleNew); // get the new position
    /* Close the gripper to grab an object */
    while(fabs(gripperAngleNew - gripperAngleOld) > 0.1) {
        gripperAngleOld = gripperAngleNew;    // update the old position
        retval = retval || getJointAngle(ROBOT_JOINT1, gripperAngleNew); // get the new position
        retval = retval || moveNB(8, NaN, 8); // move 8 degrees
        delaySeconds(1);              // closing for 1 second
        retval = retval || getJointAngle(ROBOT_JOINT1, gripperAngleNew); // get the new position
    }
    retval = retval || moveNB(8, NaN, 8);            // try to move another 8 degrees 
    delaySeconds(1);              // closing for 1 second
    retval = retval || setMovementStateNB(ROBOT_HOLD, NaN, ROBOT_HOLD); // hold the object
    return retval;
}

